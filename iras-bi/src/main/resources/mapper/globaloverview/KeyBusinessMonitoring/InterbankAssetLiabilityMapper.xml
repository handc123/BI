<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjrcu.iras.bi.globaloverview.mapper.KeyBusinessMonitoring.InterbankAssetLiabilityMapper">
    <resultMap id="InterbankAssetLiabilityResult" type="com.zjrcu.iras.bi.globaloverview.domain.model.KeyBusinessMonitoring.InterbankAssetLiability">
        <result property="id" column="id"/>
        <result property="orgName" column="org_name"/>
        <result property="regionName" column="region_name"/>
        <result property="dataDate" column="data_date"/>
        <result property="assetBalance" column="asset_balance"/>
        <result property="liabilityBalance" column="liability_balance"/>
        <result property="createTime" column="created_time"/>
        <result property="updateTime" column="updated_time"/>
    </resultMap>
    <select id="selectInterbankAssetLiability" resultMap="InterbankAssetLiabilityResult">
        select * from interbank_asset_data
        where region_name = #{globalOverviewRequestQO.regionName}
          and data_date = #{globalOverviewRequestQO.dataDate}
    </select>
</mapper>