<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjrcu.iras.bi.conversionquality.mapper.ConversionOrgRankInfoMapper">

    <resultMap type="ConversionOrgRankInfo" id="ConversionOrgRankInfoResult">
        <result property="id" column="id"/>
        <result property="dataDate" column="data_date"/>
        <result property="orgId" column="org_id"/>
        <result property="orgName" column="org_name"/>
        <result property="count" column="count"/>
        <result property="rankType" column="rank_type"/>
        <result property="rankChange" column="rank_change"/>
        <result property="createTime" column="created_time"/>
        <result property="updateTime" column="updated_time"/>
    </resultMap>

    <select id="selectConversionOrgRankInfo" resultMap="ConversionOrgRankInfoResult">
        select * from conversion_org_rank_info
        where rank_type = #{rankType}
        order by count desc
    </select>
    <select id="selectConversionOrgRankInfoPage" resultMap="ConversionOrgRankInfoResult">
        select * from conversion_org_rank_info
        where rank_type = #{rankType}
        <if test="dataDate != null and dataDate!=''">
            and data_date = #{dataDate}
        </if>
        order by count desc
    </select>
</mapper>