<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjrcu.iras.bi.conversionquality.mapper.RegionConversionQualityRankMapper">

    <resultMap type="RegionConversionQualityRank" id="RegionConversionQualityRankResult">
            <result property="dataDate" column="data_date"/>
            <result property="regionName" column="region_name"/>
            <result property="passRate" column="pass_rate"/>
            <result property="rankChange" column="rank_change"/>
            <result property="createTime" column="created_time"/>
            <result property="updateTime" column="updated_time"/>
    </resultMap>

    <sql id="selectRegionConversionQualityRankVo">
        select data_date, region_name, pass_rate, rank_change, created_time, updated_time
        from region_conversion_quality_rank
    </sql>
    <select id="selectRegionConversionQualityRank" resultMap="RegionConversionQualityRankResult">
        <include refid="selectRegionConversionQualityRankVo"/>
        order by pass_rate desc
    </select>

</mapper>