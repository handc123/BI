<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjrcu.iras.bi.conversionquality.mapper.OrgConversionTableMapper">

    <resultMap type="OrgConversionTable" id="OrgConversionTableResult">
        <result property="id" column="id"/>
        <result property="dataDate" column="data_date"/>
        <result property="orgId" column="org_id"/>
        <result property="orgName" column="org_name"/>
        <result property="unpassedTableCount" column="unpassed_table_count"/>
        <result property="passedTableCount" column="passed_table_count"/>
        <result property="createTime" column="created_time"/>
        <result property="updateTime" column="updated_time"/>
    </resultMap>

    <select id="selectOrgConversionTable" resultMap="OrgConversionTableResult">
            select * from org_conversion_table
            where org_name = #{orgName}
    </select>
</mapper>