<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjrcu.iras.bi.conversionquality.mapper.ConversionTableItemQualityMapper">

    <resultMap type="ConversionTableItemQuality" id="ConversionOrgRankInfoResult">
        <result property="id" column="id"/>
        <result property="dataDate" column="data_date"/>
        <result property="orgId" column="org_id"/>
        <result property="orgName" column="org_name"/>
        <result property="tableCode" column="table_code"/>
        <result property="tableName" column="table_name"/>
        <result property="itemCount" column="item_count"/>
        <result property="noDeviationCount" column="no_deviation_count"/>
        <result property="deviationLT2Count" column="deviation_lt_2_count"/>
        <result property="deviationGT2Count" column="deviation_gt_2_count"/>
        <result property="createTime" column="created_time"/>
        <result property="updateTime" column="updated_time"/>
    </resultMap>
    <select id="selectOrgConversionTableQuality" resultMap="ConversionOrgRankInfoResult">
        select * from conversion_table_item_quality where org_name = #{orgName}
    </select>

</mapper>